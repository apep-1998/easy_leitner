{
  "cli": {
    "version": ">= 16.27.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_FIREBASE_API_KEY": "xxxxxx",
        "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN": "xxxxxx",
        "EXPO_PUBLIC_FIREBASE_PROJECT_ID": "xxxxxx",
        "EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET": "xxxxxx",
        "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "xxxxxx",
        "EXPO_PUBLIC_FIREBASE_APP_ID": "xxxxxx",
        "EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID": "xxxxxx"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
